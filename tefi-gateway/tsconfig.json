{
  "compilerOptions": {
    "jsx": "preserve",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "allowJs": false,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "lib": ["es2015"],
  "include": ["**/*.ts", "index.ts"],
  "exclude": ["node_modules", "gateway"]
}
